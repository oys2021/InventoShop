<div class="table-responsive">
<table class="table  datanew">
<thead>
<tr>
<th>
<label class="checkboxs">
<input type="checkbox" id="select-all">
<span class="checkmarks"></span>
</label>
</th>
<th>Product Name</th>
<th>SKU</th>
<th>Created At</th>
<th>Price</th>
<th>Qty</th>
<th>Created By</th>
<th>Action</th>
</tr>
</thead>
<tbody>

    @{
        var products=ViewData["products"] as List<Product>;
    }

    @foreach(var product in products){
<tr>
<td>
<label class="checkboxs">
<input type="checkbox">
<span class="checkmarks"></span>
</label>
</td>

<td class="productimgname">
@if (!string.IsNullOrEmpty(product.ImageUrl))
    {
        <a href="javascript:void(0);" class="product-img">
            <img src="@product.ImageUrl" alt="product">
        </a>
    }
    else
    {
        <a href="javascript:void(0);" class="product-img">
            <img src="/assets/img/avatar.png" alt="default product">
        </a>
    }
<a href="javascript:void(0);">@product.ProductName</a>
</td>
<td>@product.Sku</td>
<td>@product.CreatedAt</td>
<td>@product.Price</td>
<td>@product.QuantityInStock</td>
<td>Admin</td>
<td>
    <a class="me-3" href="/admin/productdetails/@product.ProductId">
<img src="/assets/img/icons/eye.svg" alt="img">
</a>
<a class="me-3"  href="/admin/editproducts/@product.ProductId">
<img src="/assets/img/icons/edit.svg" alt="img">
</a>
</td>
</tr>
}
</tbody>
</table>
